@model Dish
<a href="/">Home</a>
<div class="container justify-content-center">
    <h1>@Model.Name</h1>
    <h3>@Model.Chef</h3>
    <hr class="my-3">
    <h5>@Model.Description</h5>
    <h4>Calories: @Model.Calories</h4>
    <h4>Tastiness: @Model.Tastiness</h4>
    <a href="/@Model.DishId/Delete" class="btn-lg btn-danger">Delete</a>
    <a href="/@Model.DishId/Edit" class="btn-lg btn-primary">Edit</a>
</div>